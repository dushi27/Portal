<h1>Listing patient_profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Firstname</th>
      <th>Last</th>
      <th>Pcprequest</th>
      <th>Profilepic</th>
      <th>Zipcode</th>
      <th>Docpref1</th>
      <th>Docpref2</th>
      <th>Doctpref3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patient_profiles.each do |patient_profile| %>
      <tr>
        <td><%= patient_profile.user_id %></td>
        <td><%= patient_profile.firstname %></td>
        <td><%= patient_profile.lastname %></td>
        <td><%= patient_profile.pcprequest %></td>
        <td><%= patient_profile.profilepic %></td>
        <td><%= patient_profile.zipcode %></td>
        <td><%= patient_profile.docpref1 %></td>
        <td><%= patient_profile.docpref2 %></td>
        <td><%= patient_profile.doctpref3 %></td>
        <td><%= link_to 'Show', patient_profile %></td>
        <td><%= link_to 'Edit', edit_patient_profile_path(patient_profile) %></td>
        <td><%= link_to 'Destroy', patient_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient profile', new_patient_profile_path %>
