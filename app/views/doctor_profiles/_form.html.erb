<%= form_tag('/doctor_profiles',:multipart => true, id: "docProfile") do %>
   <div class="form-group"> 
    <div class="container">
      <%= label_tag :firstname %><br>
      <%= text_field_tag "doctor_profile[:firstname]"%>
     </div>
   </div>
     
  <div class="form-group"> 
    <div class="container">
      <%= label_tag :lastname %><br>
      <%= text_field_tag "doctor_profile[:lastname]" %>
     </div>
   </div>
      
  <div class="form-group"> 
    <div class="container">
      <%= label_tag :about %><br>
      <%= text_area_tag "doctor_profile[:about]" %>
    </div>
  </div>
      
  <div class="form-group"> 
    <div class="container">
      <%= label_tag :profilepic %><br>
      <%= file_field_tag "doctor_profile[:profilepic]" %>           
   </div>
  </div>
      
  <div class="form-group"> 
    <div class="container">
      <%= label_tag :schedule %><br>
      <%= text_field_tag "doctor_profile[:schedule]" %>
    </div>
  </div>
      
  <div class="form-group"> 
    <div class="container">
      <%= label_tag :speciality %><br>
      <%= text_field_tag "doctor_profile[:speciality]" %>
    </div>
  </div>
      
  <div class="form-group"> 
    <div class="container">
      <%= label_tag :title %><br>
      <%= text_field_tag "doctor_profile[:title]" %>
    </div>
  </div>

 <div class="form-group"> 
  <div class="container">
    <%= label_tag :zipcode %><br>
    <%= text_field_tag "doctor_profile[:zipcode]" %>
  </div>
 </div>
      
 <div class="form-group"> 
   <div class="container">
    <%= label_tag :fee %><br>
     <%= text_field_tag "doctor_profile[:fee]" %>
   </div>
  </div>
 
      
 <div class="form-group"> 
   <div class="container">
     <%= label_tag :plan %><br>
     <%= text_field_tag "doctor_profile[:plan]" %>
   </div>
 </div>
      
 <div class="form-group"> 
  <div class="container">
    <%= label_tag :npi, 'NPI Number' %><br>
    <%= text_field_tag "doctor_profile[:npi]" %>
  </div>
</div>

  <div class="form-group">
    <%= submit_tag "Create", class: "btn-group btn btn-lg btn-success"%>
  </div>
<% end %>